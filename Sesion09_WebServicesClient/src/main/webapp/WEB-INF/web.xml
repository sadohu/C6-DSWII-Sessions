<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>OperController</servlet-name>
  	<display-name>OperController</display-name>
  	<description></description>
  	<servlet-class>ClientBasicOpers.OperController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AutorController</servlet-name>
  	<display-name>AutorController</display-name>
  	<description></description>
  	<servlet-class>Controller.AutorController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>OperController</servlet-name>
  	<url-pattern>/OperController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AutorController</servlet-name>
  	<url-pattern>/AutorController</url-pattern>
  </servlet-mapping>
</web-app>
